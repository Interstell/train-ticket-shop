@model Ticket

@{ 
    ViewBag.Title = "Билет";
}

<div class="uz-ticket container">
    <div id="middle">
        <div>
            <div>
                <div id="content">
                    <div class="cheque">
                        <div class="text">
                            <h3>
                                Посадочный документ
                            </h3>
                        </div>
                        <div class="blank electronic" data-doctype="travel">
                            <table>
                                <tr class="sizing">
                                    <td style="width: 91px"></td>
                                    <td style="width: 54px"></td>
                                    <td style="width: 79px"></td>
                                    <td style="width: 135px"></td>
                                    <td style="width: 245px"></td>
                                    <td style="width: 142px"></td>
                                    <td style="width: 171px"></td>
                                </tr>
                                <tr class="frow">
                                    <td class="nowrap" rowspan="2">
                                        TEРM.№61
                                    </td>
                                    <td class="bold arial v-top" colspan="3">
                                        ПОСАДОЧНИЙ ДОКУМЕНТ
                                    </td>
                                    <td class="bold nowrap" colspan="2">000B3AFB-<span class="uid">E7F8-465B-0001</span></td>
                                    <td class="bold arial w-right" rowspan="2">
                                        ПН:400758126555<br />
                                        ФН:3000182017<br />
                                        ЗН:ЗТ00000002<br />
                                        ФК:9789205
                                    </td>
                                </tr>
                                <tr>
                                    <td class="address v-top nowrap" colspan="3">
                                        ПАТ "УКРЗАЛІЗНИЦЯ"<br />м.Київ, вул.Тверська, буд.5
                                    </td>
                                    <td class="v-top order_num" colspan="2">#69Ц-Н1-2664121-0102</td>
                                </tr>
                                <tr>
                                    <td>МПС</td>
                                    <td class="bold arial center" colspan="5">
                                        ЦЕЙ ПОСАДОЧНИЙ ДОКУМЕНТ Є ПІДСТАВОЮ ДЛЯ ПРОЇЗДУ
                                    </td>
                                    <td class="align-right nowrap">@Model.BoughtDate</td>
                                </tr>
                                <tr>
                                    <td colspan="3">Прізвище, Ім’я</td>
                                    <td colspan="2">@Model.Surname @Model.Name</td>
                                    <td>Поїзд</td>
                                    <td class="align-right ticket_info w-right">
                                        @Model.TrainNumber ПБ
                                        @Model.TrainCategory НШ
                                    </td>
                                </tr>
                                <tr>
                                    <td class="nowrap w1" colspan="2">Відправлення</td>
                                    <td class="w1">@Model.TrainDepartureStationId.ToString().PadLeft(7,'0')</td>
                                    <td class="upper ticket_info" colspan="2">@Model.TrainPassengerDepartureStation</td>
                                    <td>Вагон</td>
                                    <td class="align-right ticket_info">@Model.CarriageNumber</td>
                                </tr>
                                <tr>
                                    <td class="nowrap" colspan="2">Призначення</td>
                                    <td>@Model.TrainArrivalStationId.ToString().PadLeft(7, '0')</td>
                                    <td class="upper ticket_info" colspan="2">@Model.TrainPassengerArrivalStation</td>
                                    <td>Місце</td>
                                    @{var ticketType = ((int)(Model.PassengerType) > 0) ? "Пільговий" : ""; }
                                    <td class="align-right ticket_info">@Model.SeatNumber @ticketType</td>
                                </tr>
                                <tr>
                                    <td colspan="3">Дата/час відпр.</td>
                                    <td class="ticket_info" colspan="2">@Model.TrainPassengerDepartureDate</td>
                                    <td>Сервіс</td>
                                    <td class="align-right">БІЛ</td>
                                </tr>
                                <tr>
                                    <td class="ch-num-l" colspan="2">Дата/час приб.</td>
                                    <td class="ch-num"></td>
                                    <td class="ticket_info" colspan="2">@Model.TrainPassengerArrivalDate</td>
                                    <td colspan="2"></td>
                                </tr>
                                <tr>
                                    <td colspan="7">
                                        ВАРТ=<b>@Model.Price.ToString("F2")ГРН</b>(КВ.16,00+ПЛ.34,09+СП.15,00+ПДВ.15,44+СТР.0,10+КЗБ.12,08)<br />
                                        В Т.Ч.
                                        БІЛ-18,00
                                        (З ПДВ)<br />
                                        @if(Model.PassengerType == PassengerType.Student) {
                                            <span>ДОКУМЕНТ ПІЛЬГОВИКА: ПІЛЬГ СТУД @Model.StudentsCardId<br /></span>
                                        }
                                        @if (Model.PassengerType == PassengerType.Kid) {
                                            <span>ДОКУМЕНТ ПІЛЬГОВИКА: СВІД НАР @Model.BirthDate<br /></span>
                                        }

                                        #СТР.ВІД Н/В 6000НЕОП.МІН. ТДВ"СТ", ДОМІНАНТА"КИЇВ,САКСАГАНСЬКОГО,119, T800507017<br />
                                        <b>ЧАС ВІДПРАВЛЕННЯ КИЇВСЬКИЙ</b>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="center" colspan="3">
                                        <img alt="QR" src="data:image/gif;base64," />
                                    </td>
                                    <td colspan="4" class="info arial">
                                        <p>Цей Посадочний документ є підставою для проїзду без звернення у касу. Посадочний документ являється розрахунковим документом.</p>
                                        <p>Повернення даного Посадочного документа можливе до відправлення поїзда.</p>
                                    </td>
                                </tr>
                            </table>
                        </div>
                        <div class="clear"></div>

                        <div class="clear"></div>
                    </div>

                    <div class="clear"></div>
                    <br />
                </div>
            </div>
        </div>
    </div>
</div>
